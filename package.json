{
  "name": "searchengineswitcher",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "lint:web": "cd build && web-ext lint && cd ..",
    "lint": "eslint **/*.{js,ts,tsx}",
    "lint:fix": "eslint **/*.{js,ts,tsx} --fix",
    "dev:build": "webpack --mode=development",
    "dev:watch": "webpack -w --mode=development",
    "dev:firefox": "concurrently -k \"npm run dev:watch\" \"web-ext run --bc\" --restart-tries 3 --restart-after 1000",
    "dev:release": "run-s release:build release:lint release",
    "ci:build": "npm run release:build",
    "ci:test": "npm run release:lint",
    "ci:release": "npm run release",
    "release:build": "webpack --mode=production",
    "release:lint": "npm run lint && cd build && web-ext lint && cd ..",
    "release": "run-s release:clean release:create-dirs release:chrome release:firefox",
    "release:create-dirs": "mkdirp release release/chrome release/firefox",
    "release:clean": "rimraf release/*",
    "release:chrome": "node zip-wrapper.js --out release/chrome/extension.zip --in build/",
    "release:firefox": "run-s release:firefox:build-ext release:firefox:copy-ext release:firefox:zip-src",
    "release:firefox:build-ext": "cd build && web-ext build --overwrite-dest && cd ..",
    "release:firefox:copy-ext": "copyfiles -f ./build/web-ext-artifacts/*.zip ./release/firefox/",
    "release:firefox:zip-src": "node zip-wrapper.js  --out release/firefox/source.zip --in . --exclude .git* release node_modules build .prettier*"
  },
  "author": "",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.51.2",
    "@eslint/compat": "^1.2.9",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.28.0",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@types/react-redux": "^7.1.34",
    "@types/webpack-env": "^1.18.8",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^9.1.2",
    "copy-webpack-plugin": "^13.0.0",
    "copyfiles": "^2.4.1",
    "cross-zip-cli": "^1.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "micromatch": "^4.0.8",
    "mkdirp": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1",
    "web-ext": "^8.7.1",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "yargs": "^18.0.0",
    "zip-dir": "^2.0.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "grommet": "^2.47.0",
    "grommet-icons": "^4.13.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-redux": "^9.2.0",
    "styled-components": "^6.1.18",
    "webext-redux": "^4.0.0"
  }
}